<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<% if @user.houses.empty?%>
	<p>No tiene nada</p>
	<%else%>
	<ul>
    <% @user.houses.each do |h|%>
			<li><%= h.numero %></li>
		<% end %>
  </ul>
<% end %>


<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
